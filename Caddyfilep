# serve from
localhost:3000

gzip
browse
ext .html
#log / stdout "{method} {uri} {latency}"
#log ./access.log

rewrite {
  regexp (^/router-redux/)
  to /docs/index.html?{query}
}

# otherwise prepend public to request
rewrite {
  regexp /router-redux/(.*)
  to /docs{1} {1} /docs/index.html?{query}
}
